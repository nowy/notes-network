  "homepage": "https://github.com/nowy/notes-network#readme",
  "devDependencies": {
    "showdown": "^1.9.1",
    "@types/node": "^14.14.28",
    "@types/showdown": "^1.9.3",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "vis-data": "^7.1.2",
    "vis-network": "^9.0.2"
  }
